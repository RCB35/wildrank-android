<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="label" format="string" />

    <declare-styleable name="JSONSerializableView">
        <attr name="label" />
        <attr name="key" format="string" />
    </declare-styleable>
    <declare-styleable name="SerializableSpinnerView">
        <attr name="values" format="reference" />
        <attr name="editable" format="boolean" />
        <attr name="defaultValue" format="string" />
    </declare-styleable>
    <declare-styleable name="DataView">
        <attr name="label" />
        <attr name="expression" format="string" />
        <attr name="format">
            <enum name="decimal" value="0"/>
            <enum name="integer" value="1"/>
            <enum name="percentage" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="WhiteboardView"></declare-styleable>

</resources>